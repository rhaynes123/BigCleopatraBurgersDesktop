@page "/confirmation"
@inject NavigationManager NavManager
<h3>Order Confirmation</h3>
<div class="card">
    <div class="card-body">
        <div class="alert alert-success" role="alert">
            <h4 class="alert-heading">Order Submitted!</h4>
            <p>Order Created Successfully Check <a href="" @onclick="GoToQueue">Order Queue</a> For More Details.</p>
            <hr>

        </div>
    </div>
</div>

@code {

    private void GoToQueue()
    {
        NavManager.NavigateTo("/queue");
    }

}

